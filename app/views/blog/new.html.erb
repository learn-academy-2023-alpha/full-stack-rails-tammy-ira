<%= stylesheet_link_tag 'application.css' %>

<h1>Add New Posts Here</h1>

<%= form_with model: @blog do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <%= form.label :content %>
    <%= form.text_field :content %>
    <%= form.submit "Add Post" %>
  <% end %>

  <p><%= link_to 'All Posts', blogs_path %></p>

  <footer> <p>Tammy & Ira Blog Page</p> 
</footer>